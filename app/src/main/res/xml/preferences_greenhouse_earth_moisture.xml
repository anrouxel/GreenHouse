<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory
        app:title="@string/preference_greenhouse_earth_moisture"
        app:iconSpaceReserved="false">

        <SeekBarPreference
            app:key="earth_moisture_min"
            app:title="@string/preference_greenhouse_earth_moisture_min"
            app:iconSpaceReserved="false"
            app:showSeekBarValue="true"/>

        <SeekBarPreference
            app:key="earth_moisture_max"
            app:title="@string/preference_greenhouse_earth_moisture_max"
            app:iconSpaceReserved="false"
            app:showSeekBarValue="true"/>

    </PreferenceCategory>

</PreferenceScreen>