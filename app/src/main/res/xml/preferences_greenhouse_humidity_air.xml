<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory
        app:title="@string/preference_greenhouse_humidity_air"
        app:iconSpaceReserved="false">

        <SeekBarPreference
            app:key="humidity_air_min"
            app:title="@string/preference_greenhouse_humidity_air_min"
            app:iconSpaceReserved="false"
            app:showSeekBarValue="true"/>

        <SeekBarPreference
            app:key="humidity_air_max"
            app:title="@string/preference_greenhouse_humidity_air_max"
            app:iconSpaceReserved="false"
            app:showSeekBarValue="true"/>

    </PreferenceCategory>

</PreferenceScreen>